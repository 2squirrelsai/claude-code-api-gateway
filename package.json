{
  "name": "claude-api-gateway",
  "version": "1.0.0",
  "description": "A robust API gateway for Claude CLI with queuing, caching, and monitoring",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "lint": "eslint src/",
    "docker:build": "docker build -t claude-api-gateway .",
    "docker:up": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.yml down",
    "docker:logs": "docker-compose -f docker/docker-compose.yml logs -f gateway"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/static": "^7.0.4",
    "@fastify/view": "^9.1.0",
    "bull": "^4.12.2",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "fastify": "^4.28.1",
    "ioredis": "^5.4.1",
    "prom-client": "^15.1.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "eslint": "^9.9.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "claude",
    "api",
    "gateway",
    "queue",
    "redis",
    "anthropic"
  ],
  "author": "",
  "license": "MIT"
}
